    "gulp": "^3.9.1",
    "gulp-requirejs": "^0.4.0",
    "gulp-tsb": "^2.0.0",
	"gulp-uglify": "^1.5.3",
	"http-server": "^0.9.0",
    "merge-stream": "^1.0.0",
    "monaco-editor-core": "^0.10.0",
    "monaco-languages": "^0.9.0",
    "monaco-typescript": "^2.1.2",
    "object-assign": "^4.1.0",
    "rimraf": "^2.5.2",
    "typescript": "^2.3.4",
    "vscode-html-languageservice": "^2.0.4",
